    },
    {
			"kind": "Exe",
			"id": "BloonsTD6.exe",
			"matching_strategy": "Equals"
		}
	],
  "monitors": [
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "BSP",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "II",
        },
        {
          "name": "III",
          "layout": "HorizontalStack",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        },
        {
          "name": "IV",
          "layout": "UltrawideVerticalStack",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        },
        {
