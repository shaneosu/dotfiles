			"id": "Photoshop.exe",
			"matching_strategy": "Equals"
		},
		{
			"kind": "Exe",
			"id": "Adobe Premiere Pro.exe",
			"matching_strategy": "Equals"
		},
		{
			"kind": "Exe",
			"id": "Illustrator.exe",
			"matching_strategy": "Equals"
		},
		{
			"kind": "Exe",
			"id": "OUTLOOK.EXE",
			"matching_strategy": "Equals"
		},
		{
			"kind": "Exe",
			"id": "ApplicationFrameHost.exe",
			"matching_strategy": "Equals"
		},
		{
			"kind": "Exe",
			"id": "vmware.exe",
