        },
        {
          "name": "VII",
          "layout": "RightMainVerticalStack",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        }
      ],
      "window_based_work_area_offset_limit": 1
    },
    {
      "workspaces": [
        {
          "name": "unnamed",
          "layout": "BSP",
          "layout_rules": {},
          "apply_window_based_work_area_offset": true
        }
      ],
      "window_based_work_area_offset_limit": 1
    }
  ],
  "window_hiding_behaviour": "Cloak",
  "monitor_index_preferences": {},
  "display_index_preferences": {}
}